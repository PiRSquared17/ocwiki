<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<subclass name="oop.data.TestStructure" discriminator-value="TestStructure"
		extends="oop.data.BaseArticle">
		<property name="type" type="java.lang.String">
			<column name="type" />
		</property>
		<property name="time" type="int">
			<column name="time" />
		</property>
		<list name="sectionStructures" inverse="false" table="TestSectionStructure"
			lazy="true" cascade="all">
			<key column="id" />
			<index column="section_index"></index>
			<many-to-many class="oop.data.SectionStructure">
				<column name="section_id"></column>
			</many-to-many>
		</list>
	</subclass>
</hibernate-mapping>
