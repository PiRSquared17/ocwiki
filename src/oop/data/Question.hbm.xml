<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="oop.data.Question" table="Question" mutable="false">
		<id name="id" type="long" access="field">
			<generator class="identity" />
		</id>
        <many-to-one name="baseResource" class="oop.data.Resource"
            access="field" fetch="select" not-null="false" cascade="all">
            <column name="base_resource" />
        </many-to-one>
        <many-to-one name="baseRevision" class="oop.data.Revision"
            access="field" fetch="select" not-null="false" cascade="all">
            <column name="base_revision" />
        </many-to-one>
		<property name="mark" type="double">
			<column name="mark" not-null="true" />
		</property>
	</class>
</hibernate-mapping>
