<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<subclass name="oop.data.TopicConstraint" extends="oop.data.Constraint"
		discriminator-value="TOP">
		<set name="topics" table="TopicConstraintTopic" fetch="join">
			<key column="constraint_id" />
			<many-to-many class="oop.data.Resource" column="topic_id" />
		</set>
	</subclass>
</hibernate-mapping>
