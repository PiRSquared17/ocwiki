<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<subclass name="oop.data.ChoiceAnswer" extends="oop.data.HistoryAnswer"
	       discriminator-value="Choice">
	    <set name="answers" table="ChoiceAnswerAnswer">
	       <key column="choice"></key>
		   <many-to-many column="answer" class="oop.data.Answer"/>
		</set>
	</subclass>
</hibernate-mapping>
