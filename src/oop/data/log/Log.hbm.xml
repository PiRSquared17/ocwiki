<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
	<class name="oop.data.log.Log" table="Log" mutable="false"
		abstract="true">
		<id name="id" column="id" access="field">
			<generator class="identity" />
		</id>
		<discriminator column="type" length="10" not-null="true"></discriminator>
		<many-to-one name="user" access="field" column="user"
			class="oop.data.User" fetch="select" not-null="true" />
		<property name="timestamp" access="field" type="java.util.Date">
			<column name="timestamp" sql-type="timestamp" not-null="true"></column>
		</property>
	</class>
</hibernate-mapping>